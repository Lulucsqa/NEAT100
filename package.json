{
  "name": "neat-trading-system",
  "version": "2.0.0",
  "description": "Advanced NEAT-based algorithmic trading system with technical and fundamental analysis",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node src/tests/run-all-tests.js",
    "test:trading": "node src/tests/TradingEnvironment.test.js",
    "test:indicators": "node src/tests/TechnicalIndicators.test.js",
    "web": "http-server -p 8080 -o",
    "web:dev": "live-server --port=8080 --open=index.html",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "build": "webpack --mode production",
    "analyze": "node scripts/analyze-performance.js",
    "backtest": "node scripts/run-backtest.js",
    "optimize": "node scripts/optimize-parameters.js"
  },
  "keywords": [
    "neat",
    "algorithmic-trading",
    "forex",
    "machine-learning",
    "genetic-algorithm",
    "technical-analysis",
    "fundamental-analysis"
  ],
  "author": "NEAT Trading Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/neat-trading-system.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/neat-trading-system/issues"
  },
  "homepage": "https://github.com/your-username/neat-trading-system#readme"
}
